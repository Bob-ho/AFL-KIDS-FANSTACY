  script.
    function reqListener() {
    if (JSON.parse(this.responseText) && JSON.parse(this.responseText).team && JSON.parse(this.responseText).team.length > 0) {
    document.querySelector("#team").style.display = "block";
    }
    else {
    document.querySelector("#noTeam").style.display = "block";
    }
    }
    var oReq = new XMLHttpRequest();
    oReq.addEventListener("load", reqListener);
    oReq.open("GET", "/userTeam");
    oReq.send();
